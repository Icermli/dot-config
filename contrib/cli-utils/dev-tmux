#!/bin/bash

tmux new-session -d -s 'icerm'
tmux rename-window 'cereal'
tmux new-window -n 'notebook'
tmux new-window -n 'shell'
tmux new-window
tmux send-keys -t 'icerm':'cereal' "conda activate cereal" Enter "clear" Enter
tmux send-keys -t 'icerm':'notebook' "conda activate myenv" Enter "clear" Enter
tmux send-keys -t 'icerm':'shell' "conda activate myenv" Enter "clear" Enter
tmux send-keys -t 'icerm' "conda activate myenv" Enter "clear" Enter
tmux -2 attach-session -d
